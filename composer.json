    },
    "require": {
        "php": ">=8.5.0",
        "ext-bcmath": "*",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-pcre": "*",
        "bower-asset/inputmask": "~5.0.9",
        "bower-asset/jquery": "~4.0.0",
        "bower-asset/punycode": "~v2.3.1",
        "bower-asset/yii2-pjax": "~2.0.8",
        "cebe/markdown": "~1.2.1",
        "ezyang/htmlpurifier": "~v4.19.0",
        "yiisoft/yii2-composer": "~2.0.11"
    },
    "autoload": {
        "psr-4": {"yii\\": ""}
    },
    "bin": [
        "yii"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "dev"
        }
    },
	"repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
	"config": {
        "allow-plugins": {
            "yiisoft/yii2-composer" : true
        },
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
	"minimum-stability": "stable"
}
